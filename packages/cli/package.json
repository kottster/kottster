{
  "name": "@kottster/cli",
  "version": "2.7.8",
  "description": "CLI for Kottster",
  "main": "dist/index.js",
  "license": "Apache-2.0",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm run clean && tsc",
    "lint": "npx eslint ./cli",
    "test": "jest --coverage",
    "prepare": "husky",
    "prepare-npm-package": "yarn add @kottster/common@latest && rimraf package-lock.json && npm run update-locks && npm run lint --fix && npm run build",
    "update-locks": "yarn install && synp --source-file yarn.lock"
  },
  "bin": {
    "kottster": "dist/index.js"
  },
  "dependencies": {
    "chalk": "^4",
    "commander": "^12.0.0",
    "cross-spawn": "^7.0.6",
    "dotenv": "^16.4.5",
    "inquirer": "^11.0.1",
    "nanospinner": "^1.1.0",
    "nodemon": "^2",
    "portfinder": "^1.0.37",
    "sync-fetch": "^0.5.2"
  },
  "devDependencies": {
    "@babel/parser": "^7.24.4",
    "@babel/traverse": "^7.24.1",
    "@babel/types": "^7.24.0",
    "@eslint/js": "^9.2.0",
    "@kottster/common": "^2.0.3",
    "@types/babel__traverse": "^7.20.5",
    "@types/cross-spawn": "^6.0.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.12",
    "@types/mock-fs": "^4.13.4",
    "@types/node": "^20.12.7",
    "@types/nodemon": "^1.19.6",
    "@types/supertest": "^6.0.2",
    "@types/sync-fetch": "^0.4.3",
    "@types/ws": "^8.5.10",
    "babel-jest": "^29.7.0",
    "eslint": "^8.56.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "mock-fs": "^5.2.0",
    "rimraf": "^5.0.5",
    "supertest": "^7.0.0",
    "synp": "^1.9.13",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.8.0",
    "vite": "^6.3.5"
  },
  "peerDependencies": {
    "@kottster/common": "^2.x",
    "vite": "^6.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kottster/kottster.git"
  },
  "bugs": {
    "url": "https://github.com/kottster/kottster/issues"
  },
  "homepage": "https://kottster.app/"
}
